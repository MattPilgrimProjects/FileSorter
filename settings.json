    "keyword": "all",

    "raw_midi_path":"Z:\\raw_midi\\",    

    "search_database": "Z:\\raw_href\\search.json",

    "live_database": "Z:\\raw_href\\clean.json",

    "live_api": "C:\\inetpub\\wwwroot\\api\\",

    "import_midi_url":{
        "freemidi":"https://freemidi.org/getter-",
        "midiworld":"https://www.midiworld.com/download/"
    },

    "stage": [

        {
                "title": "freemidi",
                "search_url": "https://freemidi.org/search?q=",
                "download_url": "https://freemidi.org/getter-",
                "href_save_location": "C:\\inetpub\\wwwroot\\live\\freemidi\\",
                "localhost": "http://localhost/live/freemidi/",
                "processed_href": {
                    "search_attribute": "a",
                    "move_to": "Z:\\raw_href\\freemidi\\processed\\",
                    "match": [
                        "title="
                    ],
                    "remove": [
                        "<a href='download3-'"
                    ],
                    "trim": ">"

                },
            "import_midi": {
                    "download_url": "https://freemidi.org/getter-",
                    "download_location": "Z:\\raw_midi\\freemidi\\"
                }
        },
        {
            "title": "midiworld",
            "search_url": "https://www.midiworld.com/search/?q=",
            "download_url": "https://www.midiworld.com/download/",
            "href_save_location": "C:\\inetpub\\wwwroot\\live\\midiworld\\",
            "localhost": "http://localhost/live/midiworld/",
            "processed_href": {
                "search_attribute": "li",
                "move_to": "Z:\\raw_href\\midiworld\\processed\\",
                "match": [
                    "download"
                ],
                "remove": [
                    "<li>",
                    "<a href='https://www.midiworld.com/download/'",
                    "<div id='audio-player'",
                    "Please install flash",
                    "\n"
                ],
                "trim": "target"
            },
            "import_midi": {
                "download_url":"https://www.midiworld.com/download/",
                "download_location": "Z:\\raw_midi\\midiworld\\"

            }
        }
    ]
